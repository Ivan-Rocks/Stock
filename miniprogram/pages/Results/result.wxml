<!--miniprogram/pages/Results/result.wxml-->
<view>
  <text>恭喜您完成测试！请将本页截图并发给管理员</text>
</view>

<view>
  <text>{{name}}你一共赚了{{profit}}元</text>
</view>

<view>
  <view style="color:green">Baisc:</view>
  <text>{{name}}|{{gender}}|{{age}}|{{diploma}}|{{occupation}}|{{income}}|{{stock_experience}}|</text>
  <text>{{investment_experience}}|{{frequency}}|{{choose1}}</text>
</view>

<view>
  <view style="color:green">Personality:</view>
  <label class="outerloop" wx:for="{{personality_answers}}" style="word-break: break-all">
    <text>{{item}}</text>
  </label>
</view>

<view>
  <view style="color:green">Simulattion:</view>
  <label class="outerloop" wx:for="{{simulation_answers}}" style="word-break: break-all">
    <view>{{item}}</view>
  </label>
</view>